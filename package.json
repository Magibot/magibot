    },
    "homepage": "https://github.com/mellomaths/magibot#readme",
    "author": {
        "name": "Matheus Mello de Lima",
        "email": "mellomatheuslima@gmail.com",
        "url": "https://github.com/mellomaths"
    },
    "devDependencies": {
